{
  "name": "elm-webpack-4-starter",
  "version": "0.0.1",
  "description": "Elm starter template with webpack 4",
  "repository": {
    "type": "git",
    "url": "https://github.com/romariolopezc/elm-webpack-4-starter"
  },
  "contributors": [
    "Romario Lopez <romariolopezc@gmail.com>",
    "Geoffrey Roguelon <geoffrey.roguelon@gmail.com>"
  ],
  "scripts": {
    "webpack": "webpack",
    "webpack-dev-server": "webpack-dev-server",
    "prod": "npm run webpack -- --env.mode production",
    "prod:analyze": "npm run prod -- --env.presets analyze",
    "prod:compress": "npm run prod -- --env.presets compress",
    "dev": "webpack-dashboard --title 'Elm Dev Server' -- npm run webpack-dev-server -- --env.mode development --hot",
    "elm-analyse": "elm-analyse -s -o",
    "preprod": "rimraf dist",
    "lint": "stylelint src/static/styles"
  },
  "license": "MIT",
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "devDependencies": {
    "autoprefixer": "^8.2.0",
    "bootstrap": "^4.1.0",
    "compression-webpack-plugin": "^1.1.11",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
    "elm": "^0.18.0",
    "elm-analyse": "^0.14.2",
    "elm-hot-loader": "^0.5.4",
    "elm-webpack-loader": "^4.5.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.0.6",
    "imagemin-webpack-plugin": "^2.1.5",
    "jquery": "^3.3.1",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.7.2",
    "optimize-css-assets-webpack-plugin": "^5.0.0",
    "popper.js": "^1.14.1",
    "postcss-loader": "^2.1.3",
    "purify-css": "^1.2.5",
    "purifycss-webpack": "^0.7.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "stylelint": "^9.2.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-webpack-plugin": "^0.10.4",
    "uglifyjs-webpack-plugin": "^1.2.6",
    "webpack": "^4.12.1",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-cli": "^3.0.8",
    "webpack-dashboard": "^2.0.0",
    "webpack-dev-server": "^3.1.1",
    "webpack-merge": "^4.1.3"
  }
}
